  "components": {
    "schemas": {
      "Book": {
        "required": [
          "isbn",
          "shelfCode"
        ],
        "type": "object",
        "properties": {
          "code": {
            "maxLength": 20,
            "type": "string",
            "nullable": true
          },
          "isbn": {
            "maxLength": 15,
            "type": "string"
          },
          "isTandon": {
            "type": "boolean"
          },
          "shelfCode": {
            "maxLength": 10,
            "pattern": "^[A-Z]{1}[A-F]{1}[0-9]{2}$",
            "type": "string"
          },
          "source": {
            "$ref": "#/components/schemas/SourceBook"
          },
          "sourceDesc": {
            "maxLength": 200,
            "type": "string",
            "nullable": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "Borrow": {
        "required": [
          "codeBook",
          "emailUser"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "emailUser": {
            "maxLength": 100,
            "type": "string"
          },
          "codeBook": {
            "maxLength": 20,
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/StatusBorrow"
          },
          "borrowAt": {
            "type": "string",
            "format": "date-time"
          },
          "dueAt": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "returnAt": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "BorrowParameter": {
        "required": [
          "borrowAt",
          "duration"
        ],
        "type": "object",
        "properties": {
          "borrowAt": {
            "type": "string",
            "format": "date-time"
          },
          "duration": {
            "maximum": 30,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "LoginParameter": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "example": "bamys2@baidu.com"
          },
          "password": {
            "type": "string",
            "example": "wWtjgOG"
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "SourceBook": {
        "enum": [
          "Buy",
          "Donated"
        ],
        "type": "string"
      },
      "StatusBorrow": {
        "enum": [
          "Reserved",
          "Borrowed",
          "Returned",
          "Cancelled"
        ],
        "type": "string"
      },
      "User": {
        "required": [
          "address",
          "email",
          "name",
          "password",
          "phoneNumber",
          "role"
        ],
        "type": "object",
        "properties": {
          "email": {
            "maxLength": 100,
            "type": "string",
            "format": "email"
          },
          "name": {
            "maxLength": 100,
            "type": "string"
          },
          "password": {
            "maxLength": 200,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,}$",
            "type": "string"
          },
          "gender": {
            "$ref": "#/components/schemas/UserGender"
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date-time"
          },
          "phoneNumber": {
            "maxLength": 20,
            "type": "string"
          },
          "address": {
            "maxLength": 300,
            "minLength": 3,
            "type": "string"
          },
          "role": {
            "$ref": "#/components/schemas/UserRole"
          }
        },
        "additionalProperties": false
      },
      "UserGender": {
        "enum": [
          "Female",
          "Male"
        ],
        "type": "string"
      },
      "UserRole": {
        "enum": [
          "Admin",
          "User",
          "Librarian"
        ],
        "type": "string"
      }
    },
